    <!-- Footer -->
    <footer>
        <div class="container">
            <div class="row">
                <div class="widget span3">
                    <h4>About this example</h4>
                    <p>An example that used ASP.NET MVC 5 for implementing a Magazine website.</p>
                    <p><a href="#">Read more...</a></p>
                </div>
                <div class="widget span3">
                    <h4>Latest Tweets</h4>
                    <div class="show-tweets"></div>
                </div>
                <div class="widget span3">
                    <h4>Flickr Photos</h4>
                    <ul class="flickr-feed"></ul>
                </div>
                <div class="widget span3">
                    <h4>Contact Me</h4>
                    <p><i class="icon-map-marker"></i> Address: 123 Dummy street, Dummy ward, Dummy district, Dummy city, Viet Nam</p>
                    <p><i class="icon-phone"></i> Phone: 0084 123 45 67 890</p>
                    <p><i class="icon-user"></i> Skype: dummy</p>
                    <p><i class="icon-envelope-alt"></i> Email: <a href="#">dummy@ymail.com</a></p>
                </div>
            </div>
            <div class="footer-border"></div>
            <div class="row">
                <div class="copyright span4">
                    <p>Copyright &copy; @DateTime.Now.Year by <a href="http://weblogs.asp.net/thangchung">thangchung</a> - All rights reserved. Template by <a href="http://azmind.com">Azmind</a>.</p>
                </div>
                <div class="social span8">
                    <a class="twitter" href="https://twitter.com/thangchung"></a>
                    <a class="github" href="https://github.com/thangchung"></a>
                    <a class="linkedin" href="http://www.linkedin.com/profile/view?id=49632040"></a>
                    <a class="facebook" href="https://www.facebook.com/thangchung"></a>
